{
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "moduleFileExtensions": ["js", "jsx", "json"],
    "testPathIgnorePatterns": ["/node_modules/", "/.undodir/"],
    "verbose": true,
    "transform": {
        "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
        "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": ["node_modules/(?!(vuejs-datepicker/dist)/)"]
}
